{
  "name": "mcomp",
  "displayName": "Mcomp",
  "description": "Syntax highlighter for Mcomp-lang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mcomp",
      "aliases": ["Mcomp", "mcomp"],
      "extensions": [".mc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mcomp",
      "scopeName": "source.mc",
      "path": "./syntaxes/mcomp.tmLanguage.json"
    }]
  },
  "publisher": "Alex Colucci"
}
